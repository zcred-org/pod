{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx ./src/main.ts",
    "dev:watch": "nodemon",
    "dev:typecheck": "tsc --noEmit",
    "dev:db:up": "docker compose -f dev/docker-compose.db-dev.yml up -d",
    "dev:db:down": "docker compose -f dev/docker-compose.db-dev.yml down",
    "drizzle": "tsx ./node_modules/drizzle-kit/bin.cjs",
    "drizzle:push": "npm run drizzle push",
    "migration:migrate": "npm run drizzle migrate",
    "drizzle:migration:generate": "npm run drizzle generate",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "keywords": [],
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@didtools/codecs": "^3.0.0",
    "@testcontainers/postgresql": "^10.13.1",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.7.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@zcredjs/core": "^1.1.3",
    "drizzle-kit": "^0.24.2",
    "jwt-decode": "^4.0.0",
    "nodemon": "^3.1.7",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.1",
    "@fastify/type-provider-typebox": "^4.1.0",
    "@keyv/postgres": "^1.4.11",
    "@sinclair/typebox": "^0.33.12",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dids": "^5.0.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.1",
    "fastify-http-errors-enhanced": "^5.0.4",
    "http-errors-enhanced": "^2.0.8",
    "key-did-provider-ed25519": "^4.0.2",
    "key-did-resolver": "^4.0.0",
    "keyv": "^4.5.4",
    "ms": "^2.1.3",
    "postgres": "^3.4.4",
    "sort-keys": "^5.1.0",
    "typed-inject": "^4.0.0",
    "uint8arrays": "^4.0.10"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/**/*"
    ],
    "ext": "ts"
  }
}
